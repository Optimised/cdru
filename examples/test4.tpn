<?xml version="1.0" encoding="UTF-8"?>
<tpns xmlns="http://mers.csail.mit.edu/tpn" xsi:schemaLocation="http://mers.csail.mit.edu/tpn http://mers.csail.mit.edu/tpn-1.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tpn>
    <id>tpn1</id>
    <name>run()</name>
    <features/>
    <start-event>ev1</start-event>
    <end-event>ev2</end-event>
    <events>
      <event>
        <id>ev1</id>
        <name>ev1</name>
        <guard>
          <boolean-constant>true</boolean-constant>
        </guard>
        <dispatch/>
      </event>
      <event>
        <id>ev2</id>
        <name>ev2</name>
        <guard>
          <boolean-constant>true</boolean-constant>
        </guard>
        <dispatch/>
      </event>
      <event>
        <id>ev3</id>
        <name>ev4</name>
        <guard>
          <boolean-constant>true</boolean-constant>
        </guard>
        <dispatch/>
      </event>
    </events>
    <temporal-constraints>
      <temporal-constraint>
        <id>tc</id>
        <name>sequence_connector</name>
        <guard>
          <boolean-constant>true</boolean-constant>
        </guard>
        <to-event>ev2</to-event>
        <from-event>ev3</from-event>
        <duration>
          <bounded-duration>
            <lower-bound>1.0</lower-bound>
            <upper-bound>1.0</upper-bound>
          </bounded-duration>
        </duration>
      </temporal-constraint>
    </temporal-constraints>
    <episodes>
      <episode>
        <id>ep</id>
        <name>user specified state constraint</name>
        <guard>
          <boolean-constant>true</boolean-constant>
        </guard>
        <to-event>ev2</to-event>
        <from-event>ev1</from-event>
        <duration>
          <set-bounded-uncertain-duration>
            <lower-bound>10.0</lower-bound>
            <upper-bound>15.0</upper-bound>
          </set-bounded-uncertain-duration>
        </duration>
        <dispatch/>
        <state-constraint>
          <wff>
            <and>
              <wff>
                <boolean-expression>
                  <condition>EQUALITY</condition>
                  <value>
                    <state-variable>sv2</state-variable>
                  </value>
                  <value>
                    <constant>d1</constant>
                  </value>
                </boolean-expression>
              </wff>
              <wff>
                <boolean-expression>
                  <condition>EQUALITY</condition>
                  <value>
                    <state-variable>sv4</state-variable>
                  </value>
                  <value>
                    <constant>d2</constant>
                  </value>
                </boolean-expression>
              </wff>
              <wff>
                <boolean-expression>
                  <condition>EQUALITY</condition>
                  <value>
                    <state-variable>sv6</state-variable>
                  </value>
                  <value>
                    <constant>d3</constant>
                  </value>
                </boolean-expression>
              </wff>
            </and>
          </wff>
        </state-constraint>
      </episode>
    </episodes>
    <chance-constraints/>
    <decision-variables/>
    <state-variables>
      <state-variable>
        <id>sv1</id>
        <name>_t1.id</name>
        <domain>
          <finite-domain>
            <value>none</value>
            <value>t3</value>
            <value>t2</value>
            <value>t1</value>
          </finite-domain>
        </domain>
      </state-variable>
      <state-variable>
        <id>sv2</id>
        <name>_t1.rm.location</name>
        <domain>
          <finite-domain>
            <value>c2</value>
            <value>c1</value>
            <value>d3</value>
            <value>d2</value>
            <value>d1</value>
          </finite-domain>
        </domain>
      </state-variable>
      <state-variable>
        <id>sv3</id>
        <name>_t2.id</name>
        <domain>
          <finite-domain>
            <value>none</value>
            <value>t3</value>
            <value>t2</value>
            <value>t1</value>
          </finite-domain>
        </domain>
      </state-variable>
      <state-variable>
        <id>sv4</id>
        <name>_t2.rm.location</name>
        <domain>
          <finite-domain>
            <value>c2</value>
            <value>c1</value>
            <value>d3</value>
            <value>d2</value>
            <value>d1</value>
          </finite-domain>
        </domain>
      </state-variable>
      <state-variable>
        <id>sv5</id>
        <name>_t3.id</name>
        <domain>
          <finite-domain>
            <value>none</value>
            <value>t3</value>
            <value>t2</value>
            <value>t1</value>
          </finite-domain>
        </domain>
      </state-variable>
      <state-variable>
        <id>sv6</id>
        <name>_t3.rm.location</name>
        <domain>
          <finite-domain>
            <value>c2</value>
            <value>c1</value>
            <value>d3</value>
            <value>d2</value>
            <value>d1</value>
          </finite-domain>
        </domain>
      </state-variable>
      <state-variable>
        <id>sv7</id>
        <name>_c1.location</name>
        <domain>
          <finite-domain>
            <value>c2</value>
            <value>c1</value>
            <value>d3</value>
            <value>d2</value>
            <value>d1</value>
          </finite-domain>
        </domain>
      </state-variable>
      <state-variable>
        <id>sv8</id>
        <name>_c1.servedBy</name>
        <domain>
          <finite-domain>
            <value>none</value>
            <value>t3</value>
            <value>t2</value>
            <value>t1</value>
          </finite-domain>
        </domain>
      </state-variable>
      <state-variable>
        <id>sv9</id>
        <name>_c2.location</name>
        <domain>
          <finite-domain>
            <value>c2</value>
            <value>c1</value>
            <value>d3</value>
            <value>d2</value>
            <value>d1</value>
          </finite-domain>
        </domain>
      </state-variable>
      <state-variable>
        <id>sv10</id>
        <name>_c2.servedBy</name>
        <domain>
          <finite-domain>
            <value>none</value>
            <value>t3</value>
            <value>t2</value>
            <value>t1</value>
          </finite-domain>
        </domain>
      </state-variable>
    </state-variables>
  </tpn>
</tpns>